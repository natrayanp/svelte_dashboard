{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "watch:tailwind": "postcss public/tailwind.css -o public/index.css -w",
    "build:tailwind": "postcss public/tailwind.css -o public/index.css",
    "watch:material": "postcss public/app.scss -o public/index1.css",
    "build": "npm run build:tailwind && routify -b && rollup -c",
    "start": "sirv public --single --host",
    "serve": "serve public -p 80",
    "watch": "rollup -c -w",
    "dev": "concurrently \"routify\" \"rollup -c -w\" \"npm run watch:tailwind\""
  },
  "devDependencies": {
    "@roxi/routify": "^2.18.2",
    "@tsconfig/svelte": "^2.0.1",
    "autoprefixer": "^10.2.6",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.7",
    "postcss": "^8.3.5",
    "postcss-cli": "^8.3.1",
    "rollup": "^2.52.2",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.41.0",
    "svelte-check": "^2.2.0",
    "svelte-preprocess": "^4.7.3",
    "tailwindcss": "^2.2.6",
    "tslib": "^2.3.0"
  },
  "dependencies": {
    "@open-wc/building-rollup": "^1.10.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@tailwindcss/forms": "^0.3.3",
    "concurrently": "^6.2.0",
    "firebase": "^8.6.8",
    "sirv-cli": "^1.0.12",
    "typescript": "^4.3.4"
  }
}
